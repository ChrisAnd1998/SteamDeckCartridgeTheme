{
    "name": "Cartridge Theme",
    "author": "ChrisAnd1998",
    "manifest_version": 3,
    "target": "Other",
	"version": "v1.1",
    "description": "This theme makes your deck feel more nostalgic. Inspired by Gameboy and Nintendo DSi Home UI.",
	"patches": {
    
	"Choose Style": {
      "type": "dropdown",
      "default": "Default",
      "values": {
        "Default": {
          "cartridge/default/default.css": [
            "QuickAccess", "SP", "MainMenu"
          ]
        },
        "Light": {
          "cartridge/light/light.css": [
            "QuickAccess", "SP", "MainMenu"
          ]
        },
        "Dark": {
          "cartridge/dark/dark.css": [
            "QuickAccess", "SP", "MainMenu"
          ]
        }
      },
      "components": []
    },
	
	"Choose Background Style": {
      "type": "dropdown",
      "default": "Default",
      "values": {
        "Default": {
          "bg/default.css": [
            "QuickAccess", "SP", "MainMenu"
          ]
        },
        "Light": {
          "bg/light.css": [
            "QuickAccess", "SP", "MainMenu"
          ]
        },
        "Dark": {
          "bg/dark.css": [
            "QuickAccess", "SP", "MainMenu"
          ]
        },
		"Green": {
          "bg/green.css": [
            "QuickAccess", "SP", "MainMenu"
          ]
        }
      },
      "components": []
    }

    
  },
    "inject": {
        "shared.css": ["QuickAccess", "SP", "MainMenu"]
    }
}